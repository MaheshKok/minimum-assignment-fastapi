{
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.10",
  "pythonPlatform": "Darwin",
  "include": ["app", "tests"],
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.10",
      "pythonPlatform": "Darwin",
      "extraPaths": [
        ".venv/lib/python3.10/site-packages"
      ]
    }
  ],
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "stubPath": ".venv/lib/python3.10/site-packages",
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    ".venv",
    "alembic_migrations"
  ]
}
